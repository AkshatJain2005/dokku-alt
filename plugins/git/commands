#!/usr/bin/env bash

source "$(dirname $0)/../dokku_common"

cat

case "$1" in
  git-hook)
    check_app_name "$2"
    echo "deploy"
    echo "$APP"
    ;;

  git-*)
    APP="$(echo "$2" | perl -pe 's/(?<!\\)'\''//g' | sed 's/\\'\''/'\''/g')"
    check_app_name "$APP"
    echo "deploy"
    echo "$APP"
    ;;
esac
